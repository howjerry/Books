# API 金鑰
ANTHROPIC_API_KEY=your-api-key-here

# 日誌級別
LOG_LEVEL=INFO

# 執行配置
MAX_PARALLEL_TASKS=3
DEFAULT_RETRY_COUNT=3

# 輸出目錄
OUTPUT_DIR=./output

# Meta Agent 配置
META_AGENT_MODEL=claude-opus-4-20250514
META_AGENT_TEMPERATURE=0.3

# Subagent 配置
ANALYSIS_AGENT_MODEL=claude-sonnet-4-20250514
GENERATION_AGENT_MODEL=claude-opus-4-20250514
VALIDATION_AGENT_MODEL=claude-sonnet-4-20250514
